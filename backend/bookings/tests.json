{
  "tests": [
    {
      "name": "OPTIONS - CORS preflight",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200,
      "expectedHeaders": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "name": "GET без авторизации - ошибка 401",
      "method": "GET",
      "path": "/",
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "POST без авторизации - ошибка 401",
      "method": "POST",
      "path": "/",
      "body": {
        "booking_type": "bath",
        "entity_id": 1,
        "booking_date": "2026-02-15",
        "start_time": "10:00",
        "end_time": "12:00"
      },
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "POST с некорректными данными - без токена возвращает 401",
      "method": "POST",
      "path": "/",
      "body": {
        "booking_type": "bath"
      },
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "PUT без авторизации - ошибка 401",
      "method": "PUT",
      "path": "/",
      "body": {
        "booking_id": 1,
        "status": "confirmed"
      },
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "DELETE без авторизации - ошибка 401",
      "method": "DELETE",
      "path": "/?booking_id=1",
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    }
  ]
}